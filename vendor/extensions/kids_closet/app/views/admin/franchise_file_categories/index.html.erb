<h1>Franchise File Categories</h1>
<% render_region :top do |top| %>
  <% top.pagination_counts do %>
    <p><%= will_paginate_record_display(@franchise_file_categories) %></p>
  <% end %>
<% end %>
<div class="list_content">
  <%= render_region :before_list %>
  <table class="franchise_file_categories_index list_table" id="franchise_file_category_index_id">
    <thead>
      <tr>
        <% render_region :list_headers do |list_headers| %>
          <% list_headers.category_title_header do %><th>Title</th><% end %>
          <% list_headers.actions_column_header do %><% if @admin %><th>Actions</th><% end %><% end %>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% if @franchise_file_categories.length == 0 %>
        <tr><td colspan="<%= admin_interface.franchise_file_categories.index.list_data.length %>" class="no_entries">No File Categories</td></tr>
      <% else %>
         <%= render :partial => 'franchise_file_category', :collection => @franchise_file_categories %>
      <% end %>
    </tbody>
  </table>
  <% render_region :after_list do |after_list| %>
    <% after_list.pagination_links do %>
      <%= will_paginate_links(@franchise_file_categories) %>
    <% end %>
  <% end %>
  <div class="action_row">
    <% render_region :action_row do |action_row| %>
      <% action_row.edit_form do %>
        <% form_for [:admin, @franchise_file_category], :html => {:class => 'semantic-form'} do |f| %>
          <%= render :partial => 'form', :locals => { :f => f, :save_label => 'Create' } %>
        <% end %><br />
      <% end %>
    <% end %>
  </div>
</div>
