<h3><b>Log Report for <%= @transaction_import.site_asset.nil? ? "Legacy Import #{@transaction_import.id}" : @transaction_import.site_asset.display_name %></b></h3>
<div class="list_content">
  <%= render_region :before_list %>
  <table class="site_asset_index list_table" id="site_asset_index_id">
    <thead>
      <tr>
        <% render_region :list_headers do |list_headers| %>
          <% list_headers.item_number_column_header do %><th>Item #</th><% end %>
          <% list_headers.consignor_number_column_header do %><th nowrap>Consignor #</th><% end %>
          <% list_headers.consignor_name_column_header do %><th>Name</th><% end %>
          <% list_headers.error_message_column_header do %><th>Message</th><% end %>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% if @log_items.length == 0 %>
        <tr><td colspan="<%= admin_interface.transaction_imports.index.list_data.length %>" class="no_entries">No Log Entries Created for this Import</td></tr>
      <% else %>
         <%= render :partial => 'log_item', :collection => @log_items %>
      <% end %>
    </tbody>
  </table>
  <div class="action_row">
    <% render_region :action_row do |action_row| %>
      <% action_row.transaction_import_path do %>
        <%= link_to 'Back', admin_sale_transaction_imports_url(@transaction_import.sale) %>
      <% end %>
    <% end %>
  </div>
</div>
