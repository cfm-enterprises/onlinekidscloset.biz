<td class="name"><%= @sale_consignor_sign_up.franchise_profile.profile_id %></td>
<td class="name"><%= @sale_consignor_sign_up.franchise_profile.profile.items_coming %></td>
<td class="name"><%= @sale_consignor_sign_up.franchise_profile.profile.sort_column %></td>
<td class="name"><%= @sale_consignor_sign_up.franchise_profile.profile.full_name %></td>
<td><%= link_to @sale_consignor_sign_up.franchise_profile.profile.email, "mailto:#{@sale_consignor_sign_up.franchise_profile.profile.email}" %></td>
<td><%= @sale_consignor_sign_up.franchise_profile.profile.phone.as_phone %></td>
