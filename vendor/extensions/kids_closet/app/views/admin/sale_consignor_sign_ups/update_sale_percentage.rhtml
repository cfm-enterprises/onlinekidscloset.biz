<td class="actions">
  <%= link_to 'Disclaimer', consignor_contract_admin_sale_consignor_sign_up_url(@sale_consignor_sign_up), :popup => ['consignor_disclaimer', 'height=650,width=800'] %> | 
  <%= link_to 'Edit', edit_admin_sale_consignor_sign_up_url(@sale_consignor_sign_up) %> | <%= link_to "Remove", admin_sale_consignor_sign_up_url(@sale_consignor_sign_up), :confirm => "Are you sure?", :method => :delete %>
