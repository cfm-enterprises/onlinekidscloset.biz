<h1>Franchise Photos for <%= @franchise_photo.franchise.franchise_name %></h1>
<% render_region :top do |top| %>
  <% top.pagination_counts do %>
    <p><%= will_paginate_record_display(@franchise_photos) %></p>
  <% end %>
<% end %>
<div class="list_content">
  <%= render_region :before_list %>
  <table class="site_asset_index list_table" id="site_asset_index_id">
    <thead>
      <tr>
        <% render_region :list_headers do |list_headers| %>
          <% list_headers.sort_order_column_header do %><th>#</th><% end %>
          <% list_headers.icon_column_header do %><th>&nbsp;</th><% end %>
          <% list_headers.name_column_header do %><th>Name</th><% end %>
          <% list_headers.caption_column_header do %><th>Caption</th><% end %>
          <% list_headers.show_on_slider_column_header do %><th>Show on Home Page Slider</th><% end %>
          <% list_headers.actions_column_header do %><th>Actions</th><% end %>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% if @franchise_photos.length == 0 %>
        <tr><td colspan="<%= admin_interface.franchise_photos.index.list_data.length %>" class="no_entries">No Franchise Photos</td></tr>
      <% else %>
         <%= render :partial => 'franchise_photo', :collection => @franchise_photos %>
      <% end %>
    </tbody>
  </table>
  <% render_region :after_list do |after_list| %>
    <% after_list.pagination_links do %>
      <%= will_paginate_links(@franchise_photos) %>
    <% end %>
  <% end %>
  <div class="action_row">
    <% render_region :action_row do |action_row| %>
      <% action_row.index_form do %>
        <% form_for [:admin, @franchise_photo.franchise, @franchise_photo], :html => {:class => 'semantic-form', :multipart => true} do |f| %>
          <%= render :partial => 'form_index', :locals => { :f => f } %>
        <% end %><br />
      <% end %>
    <% end %>
  </div>
</div>
