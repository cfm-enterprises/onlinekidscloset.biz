<div class="list_content" id="promo_codes_tab_id">
  <table class="list_table">
    <thead>
      <tr>
        <% render_region :promo_codes_list_headers do |list_headers| %>
          <% list_headers.promo_code_column_header do %><th>Promo Code</th><% end %>
          <% list_headers.count_column_header do %><th>Count</th><% end %>
          <% list_headers.actions_column_header do %><th>Actions</th><% end %>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% if @promo_codes.length == 0 %>
        <tr><td colspan="<%= admin_interface.sales.promo_codes.promo_codes_list_data.length %>" class="no_entries">No Promo Codes Used for this Sale</td></tr>
      <% else %>
         <%= render :partial => 'promo_code', :collection => @promo_codes %>
      <% end %>
    </tbody>
  </table>
</div>