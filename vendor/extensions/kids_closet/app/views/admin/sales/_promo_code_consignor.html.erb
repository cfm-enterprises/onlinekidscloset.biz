<tr id="promo_code_consignor-<%= promo_code_consignor.franchise_profile.id.to_s %>" class="promo_code_consignor_row list_row">
  <% render_region :list_data, :locals => { :promo_code_consignor => promo_code_consignor } do |list_data| %>
    <% list_data.consignor_number_column do %>
      <td class="name"><%= promo_code_consignor.franchise_profile.profile_id %></td>
    <% end %>
    <% list_data.name_column do %>
      <td><%= promo_code_consignor.franchise_profile.profile.full_name %></td>
    <% end %>
    <% list_data.email_column do %>
      <td><%= link_to promo_code_consignor.franchise_profile.profile.email, "mailto:#{promo_code_consignor.franchise_profile.profile.email}" %></td>
    <% end %>
  <% end %>
</tr>