<div class="list_content" id="franchise_owners_tab_id">
  <table class="list_table">
    <thead>
      <tr>
        <% render_region :franchise_owner_list_headers do |list_headers| %>
          <% list_headers.franchise_owner_name_column_header do %><th>Owner</th><% end %>
          <% list_headers.franchise_owner_email_column_header do %><th>Email</th><% end %>
          <% list_headers.franchise_owner_phone_column_header do %><th>Phone</th><% end %>
          <% list_headers.actions_column_header do %><th>Actions</th><% end %>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% if @franchise_owners.length == 0 %>
        <tr><td colspan="<%= admin_interface.franchises.show.franchise_owner_list_data.length %>" class="no_entries">No Owners Assigned to this Franchise</td></tr>
      <% else %>
         <%= render :partial => 'franchise_owner', :collection => @franchise_owners %>
      <% end %>
    </tbody>
  </table>
  <% render_region :after_list do |after_list| %>
    <% after_list.pagination_links do %>
      <%= empty_pagination_div %>
    <% end %>
  <% end %>
  <div class="action_row">
    <% render_region :franchise_owners_action_row do |action_row| %>
      <% action_row.new_franchise_owner_profile do %>
        <%= link_to 'Add Franchise Owner', new_admin_franchise_franchise_owner_profile_path(@franchise.id) %>
      <% end %>
    <% end %>
  </div>
</div>