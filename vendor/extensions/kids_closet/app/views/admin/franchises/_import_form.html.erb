<% field_set_tag "Import Members" do -%>
  <% render_region :form_top, :locals => { :f => f } do |form_top| %>
    <% form_top.edit_errors do %>
      <%= error_messages_for :profile, :franchise_profile, :site_asset %>
    <% end %>
  <% end %>
  <% render_region :form, :locals => { :f => f } do |form| %>
    <% fields_for @site_asset do |site_asset_form| %>
      <% render_region :site_asset_fields do |site_asset_fields| %>
        <% site_asset_fields.edit_filename do %>
          <%= site_asset_form.file_field :asset, :label => "Member CSV:", :size => 25, :required => true, :help => "Please make sure there are NO SPACES on the file name" %>
        <% end %>      
      <% end %>
    <% end %>
    <% fields_for @franchise_profile do |franchise_profile_form| %>    
      <% render_region :franchise_profile_fields do |franchise_profile_fields| %>
        <% franchise_profile_fields.edit_franchise_id do %>
          <%= franchise_profile_form.hidden_field :franchise_id %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
  <% render_region :form_bottom, :locals => { :f => f } do |form_bottom| %>
    <% form_bottom.edit_buttons do %>
      <%= f.submit_or_back "Import", admin_franchise_url(@franchise_profile.franchise) %>
    <% end %>
  <% end %>
<% end %>