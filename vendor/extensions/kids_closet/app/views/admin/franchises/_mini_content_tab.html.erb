<div class="list_content" id="mini_content_tab_id">
  <% render_region :mini_content_list_content do |list_headers| %>
    <% list_headers.content_form_header do %>
      <h3>Mini Site Page Content:</h3>
    <% end %>
    <% list_headers.mini_content_form do %>
      <% form_for [:admin, @franchise], :url => {:action => 'update_content'} do |f| %>
        <%= render :partial => 'content_form', :locals => { :f => f } %>
      <% end %>
    <% end %>
  <% end %>
</div>
